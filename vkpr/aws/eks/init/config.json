{
  "inputs": [
    {
      "tutorial": "Specifies the EKS Cluster name",
      "label": "EKS cluster name:",
      "name": "cluster_name",
      "type": "text",
      "default": "eks-sample",
      "required": true,
      "cache": {
        "active": true,
        "qty": 2,
        "newLabel": "Type another EKS cluster name"
      },
      "pattern": {
        "regex": "^([A-Za-z0-9-]+)$",
        "mismatchText": "Invalid Cluster name"
      }
    },
    {
      "tutorial": "Specifies the EKS Kubernetes Version",
      "label": "K8s version",
      "name": "k8s_version",
      "type": "text",
      "default": "1.23",
      "required": true,
      "items": [
        "1.24",
        "1.23",
        "1.22",
        "1.21",
        "1.20"
      ]
    },
    {
      "tutorial": "Specifies the EKS Node instance type ",
      "label": "Choose nodes instance type:",
      "name": "cluster_node_instance_type",
      "type": "text",
      "default": "t3",
      "required": true,
      "items": [
        "t3",
        "t3a",
        "t4g",
        "m5",
        "m5a",
        "m6i",
        "m6g"
      ]
    },
    {
      "tutorial": "Specifies the EKS Node instance size: ",
      "condition": {
        "variable": "cluster_node_instance_type",
        "operator": "containsAny",
        "value": "t"
      },
      "label": "Choose nodes instance size: ",
      "name": "cluster_node_instance_size",
      "type": "text",
      "default": "small",
      "items": [
        "small",
        "medium",
        "large"
      ]
    },
    {
      "tutorial": "Specifies the EKS Node instance size: ",
      "condition": {
        "variable": "cluster_node_instance_type",
        "operator": "containsAny",
        "value": "m"
      },
      "label": "Choose nodes instance size: ",
      "name": "cluster_node_instance_size",
      "type": "text",
      "default": "large",
      "items": [
        "large",
        "xlarge",
        "2xlarge"
      ]
    },
    {
      "tutorial": "Specifies the number of Nodes",
      "label": "Desired number of nodes:",
      "name": "cluster_size",
      "type": "text",
      "default": "1",
      "required": true,
      "pattern": {
        "regex": "^([0-9])$",
        "mismatchText": "Invalid number of nodes"
      }
    },
    {
      "tutorial": "Specifies the Node Group capacity type",
      "label": "Choose the capacity type from the nodes:",
      "name": "capacity_type",
      "type": "text",
      "default": "on_demand",
      "required": true,
      "items": [
        "on_demand",
        "spot"
      ]
    },
    {
      "tutorial": "Specifies the Git provider",
      "label": "Git Provider:",
      "name": "provider",
      "type": "text",
      "default": "user",
      "required": true,
      "items": [
        "Gitlab",
        "Github"
      ]
    },
    {
      "tutorial": "Specifies where you want to store the TF state",
      "condition": {
        "variable": "provider",
        "operator": "==",
        "value": "Github"
      },
      "label": "Choose where to store the state of Terraform:",
      "name": "terraform_state",
      "type": "text",
      "default": "s3",
      "required": true,
      "items": [
        "s3",
        "terraform_cloud"
      ]
    },
    {
      "tutorial": "Specifies where you want to store the TF state",
      "condition": {
        "variable": "provider",
        "operator": "==",
        "value": "Gitlab"
      },
      "label": "Choose where to store the state of Terraform:",
      "name": "terraform_state",
      "type": "text",
      "default": "gitlab",
      "required": true,
      "items": [
        "gitlab",
        "s3",
        "terraform_cloud"
      ]
    },
    {
      "tutorial": "Specifies the project location to create the project",
      "label": "Project location:",
      "name": "project_location",
      "type": "text",
      "default": "user",
      "required": true,
      "items": [
        "user",
        "groups"
      ]
    },
    {
      "tutorial": "Specifies your group path",
      "condition": {
        "variable": "project_location",
        "operator": "==",
        "value": "groups"
      },
      "label": "Location path:",
      "name": "project_location_path",
      "type": "text",
      "required": false,
      "cache": {
        "active": true,
        "qty": 2,
        "newLabel": "Type another location path"
      }
    },
    {
      "tutorial": "Specifies your Gitlab Username",
      "condition": {
        "variable": "terraform_state",
        "operator": "==",
        "value": "Gitlab"
      },
      "name": "gitlab_username",
      "type": "CREDENTIAL_GITLAB_USERNAME"
    },
    {
      "tutorial": "Specifies your Gitlab Access-Token",
      "condition": {
        "variable": "terraform_state",
        "operator": "==",
        "value": "Gitlab"
      },
      "name": "gitlab_token",
      "type": "CREDENTIAL_GITLAB_TOKEN"
    },
    {
      "tutorial": "Specifies the gitlab url",
      "condition": {
        "variable": "provider",
        "operator": "==",
        "value": "Gitlab"
      },
      "name": "gitlab_url",
      "type": "CREDENTIAL_GITLAB_URL"
    },
    {
      "tutorial": "Specifies your Github Username",
      "condition": {
        "variable": "provider",
        "operator": "==",
        "value": "Github"
      },
      "name": "github_username",
      "type": "CREDENTIAL_GITHUB_USERNAME"
    },
    {
      "tutorial": "Specifies your Github personal access token",
      "condition": {
        "variable": "provider",
        "operator": "==",
        "value": "Github"
      },
      "name": "github_token",
      "type": "CREDENTIAL_GITHUB_TOKEN"
    },
    {
      "tutorial": "Specifies the AWS Access Key Credential",
      "name": "aws_access_key",
      "type": "CREDENTIAL_AWS_ACCESSKEYID"
    },
    {
      "tutorial": "Specifies the AWS Secret Key Credential",
      "name": "aws_secret_key",
      "type": "CREDENTIAL_AWS_SECRETACCESSKEY"
    },
    {
      "tutorial": "Specifies the AWS Region to set the environment",
      "name": "aws_region",
      "type": "CREDENTIAL_AWS_REGION"
    },
    {
      "tutorial": "Specifies your Terraform Cloud Token",
      "condition": {
        "variable": "terraform_state",
        "operator": "==",
        "value": "terraform_cloud"
      },
      "name": "terraformcloud_api_token",
      "type": "CREDENTIAL_TERRAFORMCLOUD_TOKEN",
      "required": false
    },
    {
      "tutorial": "Specifies the S3 bucket",
      "condition": {
        "variable": "terraform_state",
        "operator": "==",
        "value": "s3"
      },
      "name": "s3_bucket",
      "type": "CREDENTIAL_S3_BUCKET"
    },
    {
      "tutorial": "Specifies the S3 key",
      "condition": {
        "variable": "terraform_state",
        "operator": "==",
        "value": "s3"
      },
      "name": "s3_key",
      "type": "CREDENTIAL_S3_KEY"
    }
  ],
  "template": "shell-bat",
  "templateRelease:": "2.16.2"
}